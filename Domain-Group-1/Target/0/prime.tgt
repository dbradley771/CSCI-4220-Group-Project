int i;
int m = 0;
int flag = 0;

int n = 4;
m = n div 2;
if(n == 0 or n == 1) then {
    print(false);
} else {
    for(i = 2; i <= m and flag == 0; i++) {
        if(n mod i == 0) then {
            print(false);
            flag = 1;
        };
    };
    
    if(flag == 0) then {
        print(true);
    };
};